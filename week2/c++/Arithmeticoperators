#include<iostream> 
using namespace std; 
int main() 
{ 
int a,b; 
char choice; 
cout << "Enter a,b values : "; 
cin >> a >> b; 
cout << "Enter arithmetical operation : "; 
cin >> choice; 
switch(choice) 
{ 
case '+' : cout << a+b; 
break; 
case '-' : cout << a-b; 
break; 
case '*' : cout << a*b; 
break; 
case '/' : cout << a/b;
}
break; 
case '%' : cout << a%b; 
break; 
}
}
